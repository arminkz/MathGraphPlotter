<AKPMGP>
<rv>10.5</rv>
<set>
drawmode=2D
acc=50
</set>
<eq>
y=a*x^2+b*x+c;1;[-inf,+inf];{};sc:#FF0000FF;2;1;1
</eq>
<var>
a=1;[-10,10];0.1;0;0;0
b=-3;[-10,10];0.1;0;0;0
c=2;[-10,10];0.1;0;0;0
</var>
<point>
P1((-b-Sqr((b^2)-(4*a*c)))/(2*a),0,0);sc:#FFFF0000;1;0
P2((-b+Sqr((b^2)-(4*a*c)))/(2*a),0,0);sc:#FFFF0000;1;0
</point>
